package com.jagannadh;

public class Sum {
	public static double sumOfDoubles(double... values) {
		double a = 0.0;
	 if(values.length!=0) 
	 for(double value:values) 
	  a+=value;
	 else 
		 System.out.println("Operation Fail Due to you did not pass Values");
	return a;
	}

	public static void main(String[] args) {
   System.out.println(sumOfDoubles(1.2,4,5,6.89,0.0009,9.809));
	}
}
