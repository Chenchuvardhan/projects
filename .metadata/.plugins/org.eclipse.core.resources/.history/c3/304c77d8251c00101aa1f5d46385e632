package com.jse;
import java.util.*;
import java.util.concurrent.ArrayBlockingQueue;


class Test {
    public static void main(String[] args) {
    	Queue<Integer> queue = new ArrayBlockingQueue<>(3);
    	queue.offer(1);
    	queue.offer(2);
    	queue.offer(3);
    	queue.add(4);
    	queue.poll();
    	queue.offer(5);
    	queue.poll();
    	queue.offer(6);
    	System.out.println(queue); 
    }
}

